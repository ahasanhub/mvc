@model IList<string>
@{
    ViewBag.Title = "Download";
}

<h2>Download</h2>
<table>
    <tr>
        <th>File Name</th>
        <th>Link</th>
    </tr>
    @for (var i = 0; i <= Model.Count - 1; i++)
    {
        <tr>
            <td>@Model[i].ToString() </td>
            <td>@Html.ActionLink("Download", "ForceDownload", new { fileName = @Model[i].ToString() }) </td>
        </tr>
    }
</table>

